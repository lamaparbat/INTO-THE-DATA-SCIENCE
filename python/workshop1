    {
      "cell_type": "code",
      "source": [
        "# function definition\n",
        "def add(a, b):\n",
        "  return a+b;\n",
        "\n",
        "def subtract(a, b):\n",
        "  return a-b;\n",
        "\n",
        "def multiply(a, b):\n",
        "  return a*b;\n",
        "\n",
        "def divide(a, b):\n",
        "  return a/b;\n",
        "\n",
        "\n",
        "# function calling() / envoke()\n",
        "print(add(1,2))\n",
        "print(subtract(3,1))\n",
        "print(multiply(3,2))\n",
        "print(divide(4,2))"
